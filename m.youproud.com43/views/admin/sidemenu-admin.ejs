<div class="header-mobile ">
	<div class="header-logo">
		<a href="/profile" id="header__user-info" class="sidemenu-header">
			<figure>
				<img class="userPhoto" src="<%= userPhoto %>">
			</figure>
			<p class="userName">@<%= userNickname %></p>
		</a>
	</div>
	<div class="hamburger">
		<div class="h-line h-top"></div>
		<div class="h-line h-middle"></div>
		<div class="h-line h-bottom"></div>
		<div id="total-notifications-count" class="unreaded-messages-count" style="display: none;"></div>
	</div>
    <div id="chat-header" class="center-content">
        <!--block user button-->
        <div class="block-status-btn chat block" id="blacklist-toggle" style="display: none">
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16">
                <circle cx="8" cy="8" r="7" fill="none" stroke-linecap="round" stroke-miterlimit="10"
                        stroke-width="2"></circle>
                <line x1="3.05" y1="3.05" x2="12.95" y2="12.95" fill="none" stroke-miterlimit="10"
                      stroke-width="2"></line>
            </svg>
            <span>block</span>
            <span>unblock</span>
        </div>

        <!--profile info-->
        <div class="header-profile-info" <% if(page == "chat") { %> style="display: none" <%}%>>
            <figure class="user-avatar">
                <img src="<%= userPhoto %>">
            </figure>
            <div class="header-profile-name">
                <p>
                    <span class="recipient-id"><%= userId %></span>
                    @<span class="nickname" style="padding: 0; font-size: 14px;"><%= userNickname %></span>
				</p>
                <p class="another-info">
                    <% if(userAge) { %>
                    <!--<span class="recipient-age"><%= userAge %> years</span>-->
                    <% } %>
                    <% if(userCity) { %>
                    <div class="recipient-residence">
						<span class="user-city"><%= userCity %>, <%= userCountry %></span>
				    </div>
                    <% } %>
                </p>
            </div>
        </div>

		<% if(page == "chat") { %>
		<div class="read-all-btn" style="display: none;">Read all</div>
		<% } else if(page != "travelers") { %>
		<a href="/profile/edit"><svg class="edit-profile" xmlns="http://www.w3.org/2000/svg" width="20.098" height="20" viewBox="0 0 20.098 20"><title>edit</title><path d="M12.5,3.338l4.088,4.088L6.237,17.775,2.151,13.687Zm7.191-.986L17.865.528a1.809,1.809,0,0,0-2.556,0L13.563,2.275l4.088,4.088,2.037-2.037a1.394,1.394,0,0,0,0-1.974ZM.011,19.433a.465.465,0,0,0,.563.553l4.556-1.1L1.044,14.793Zm0,0" fill="#fff"/></svg></a>
		<% } %>
	</div>

	<div class="side-menu">
		<div class="side-menu-divider"></div>
		<a href="/bot-chat">
			<div class="side-menu-item <%= (page == "bot-chat") ? "active" : "" %>">

				<div class="side-menu-item-ico-holder">
					<svg class="stroke-ico" xmlns="http://www.w3.org/2000/svg" width="33" height="33" viewBox="0 0 33 33"><title>
							in-chat</title>

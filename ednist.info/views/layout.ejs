          content="<%= (typeof metaData !== 'undefined' && metaData.image) ?
                  metaData.image :
                  'https://www.ednist.info/media/settings/soclogo.jpg' %>"
    />
    <link rel="image_src"
          href="<%= (typeof metaData !== 'undefined' && metaData.image) ?
                  metaData.image :
                  'https://www.ednist.info/media/settings/soclogo.jpg' %>"
    >


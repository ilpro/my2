        <div class="news-feed_sidebar sid2">
            <%thirdFeedArr.splice(0, 5).forEach(newsObj => { %>

            <div class="news-feed_item">
                <a href="/news/<%= newsObj.newsId %>">
                  <div class="news-feed_images" style="background-image: url(https://www.ednist.info/media/images/<%= newsObj.newsId %>/big/400.jpg); background-size: cover; background-position: 50%;">

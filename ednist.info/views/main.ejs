<div class="wrapper">
  <div class="all-news">
    <section>
      <div class="news">
        <!-- Swiper -->
  <div class="main-news-container">
      <div class="swiper-container">
          <div class="swiper-wrapper">
                <% listMain.forEach(newsObj => { %>
                  <div class="about-news swiper-slide">
                    <a href="/news/<%= newsObj.newsId %>">
                    <div class="about-images" style="background-image: url(https://www.ednist.info/media/images/<%= newsObj.newsId %>/big/<%= newsObj.newsImage %>); background-size: cover; background-position: 50%">
                      <div class="about-grad"></div>
                    </div>
                    </a>
                    <span class="signature-images"><a href="/news/<%= newsObj.category %>"><%= newsObj.categoryUkr %></a></span>
                      <div class="news-style" >
                          <div class="hashtag-block">
                            <div class="hashtag">
                              <ul>
                                <% newsObj.newsTags.forEach(tag => { %>
                                <li><a href="/tags/<%=tag%>">#<%=tag%></a></li>
                                <% }) %>
                              </ul>
                            </div>
                          </div>
                          <a href="/news/<%= newsObj.newsId %>"><div class="text-block">
                              <p><%= newsObj.newsHeader %></p>
                              <p><%= newsObj.newsSubheader%></p>
                                                      <p>
                                <span class="date"><%=newsObj.time%></span>
                                                      </p>
                            </div></a>
                    </div>
                  </div>
                <%})%>
          </div>
          <div class="swiper-pagination"></div>
      </div>
  </div></div>
    </section>


    <!-- POPULAR -->
    <div class="popular-section">
      <aside class="news-popular-container">
        <div class="popular"><a href="#">Популярне за добу</a></div>
        <% listPopular.forEach(newsObj => { %>
        <div class="popular-item section-item_style">
          <a href="/news/<%= newsObj.newsId %>"></a>
          <div class="popular-item_style popular-item_text">
            <div class="hashtag">
              <ul>
                <% newsObj.newsTags.forEach(tag => { %>
                  <li><a href="/tags/<%=tag%>">#<%=tag%></a></li>
                <% }) %>
              </ul>
            </div>
            <div class="popular-link_news">
              <a href="/news/<%= newsObj.newsId %>"><%= newsObj.newsHeader %></a>
            </div>
            <div class="popular-text_section">
                <a href="/news/<%= newsObj.category %>"><%= newsObj.categoryUkr %></a>
                <span class="date">
                    <%=newsObj.time%>
                </span>
            </div>
          </div>
        </div>
        <%})%>
        <div class="more">
          <a href="/news/popular">Більше</a>
          <span class="more-arrow">&rarr;</span>
        </div>
      </aside>
    </div>
  </div>

  <!-- DOSSIER -->
    <div class="dossier">
      <div class="dossier-container">
          <%listRandomDossiers.forEach(dossier => { %>
          <figure>
                <a href="/dossier/<%= dossier.dossierId %>">
                  <div class="images-block">
                    <div class="dossier-images" style="background-image: url(https://www.ednist.info/media/brand/<%= dossier.dossierId %>/main/400.jpg); background-size: cover; background-position: 50%">
                      <div class="dossier-grad"></div>
                    </div>
                  </div>
                </a>
                
              <div class="signature-block">
                <span class="signature-images"><a href="/dossier/<%= dossier.category%>">Досьє</a></span>
              </div>

              <figcaption>
                  <a href="/dossier/<%= dossier.dossierId %>">
                    <div class="block-text">
                      <div class="dossier-text">
                        <p><%= dossier.dossierName %></p>
                      </div>
                    </div>
                  </a>
              </figcaption>
          </figure>
          <%})%>
      </div>
    </div>

  <!-- NEWS FEED -->

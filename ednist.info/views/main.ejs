  <a href="/news" class="test-button">
    <button>Читати більше</button>
  </a>
</div>

  <!-- EXCLUSIVE NEWS -->

  <div class="popular-news-wrapper">
      <div class="wrapper">
        <div class="exclusive-container">
      <div class="exclusive">
          <%listPolitics.forEach(newsObj => { %>
          <div class="exclusive-item">
              <a href="/news/<%= newsObj.newsId %>">
                <div class="exclusive-images" style="background-image: url(https://www.ednist.info/media/images/<%= newsObj.newsId %>/big/<%= newsObj.newsImage %>); background-size: cover; background-position: 50%">
                <div class="exclusive-grad"></div>
                </div>
              </a>
              <span class="signature-images">
                  <a href="/news/<%= newsObj.category %>"><%= newsObj.categoryUkr %></a>
              </span>
              <article>
                  <div class="exclusive-item_text">
                      <div class="hashtag-block">
                        <div class="hashtag">
                            <ul>
                                <% newsObj.newsTags.forEach(tag => { %>
